{
  "consumer": {
    "name": "consumer-one"
  },
  "interactions": [
    {
      "description": "get single book",
      "providerStates": [
        {
          "name": "Getting book with any ID returns Clean Code"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/books/b3fc0be8-463e-4875-9629-67921a1e00f4"
      },
      "response": {
        "body": {
          "authors": [
            "Robert C. Martin",
            "Dean Wampler"
          ],
          "isbn": "9780132350884",
          "title": "Clean Code"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.authors[0]": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.authors[1]": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.isbn": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.title": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-jvm": {
      "version": "4.2.5"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "provider"
  }
}
